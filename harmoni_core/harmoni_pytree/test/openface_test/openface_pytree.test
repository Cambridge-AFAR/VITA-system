<launch>
    <include file="$(find harmoni_openface)/launch/openface_service.launch">
    </include>

    <param name="instance_id" value="default"/>
    <node pkg="harmoni_pytree" type="openface_service.py" name="harmoni_openface_default_pytree" output="screen"/>

    <param name="test_openface_input" value="$(find harmoni_face_detect)/test/test_data/composer.jpg"/>
    <test test-name="test_openface" pkg="harmoni_pytree" type="rostest_openface_pytree.py" />
</launch>