<!-- This is an example launch file for how a ROS application could use a node -->
<launch>
	<rosparam file="$(find harmoni_bot)/config/configuration.yaml" subst_value="True"/>
    <param name="instance_id" value="default"/>
    <param name="name_bot" value="chat_gpt"/>
    <node pkg="harmoni_bot" type="chat_gpt_service.py" name="harmoni_bot_chatgpt_default" output="screen"/>
    <param name="test_bot_input" value="Voglio comprare dei fiori"/>
    <test test-name="test_chatgpt" pkg="harmoni_pytree" type="rostest_chatgpt_pytree.py" />
</launch>
