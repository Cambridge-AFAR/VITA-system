<launch>
    <include file="$(find harmoni_opensmile)/launch/opensmile_service.launch">
    </include>

    <param name="instance_id" value="default"/>
    <node pkg="harmoni_pytree" type="opensmile_service.py" name="harmoni_opensmile_default_pytree" output="screen"/>

    <param name="test_opensmile_input" value="$(find harmoni_stt)/test_data/hello.wav"/>
    <test test-name="test_opensmile" pkg="harmoni_pytree" type="rostest_opensmile_pytree.py" />
</launch>